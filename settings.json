{
    "editor.renderWhitespace": "boundary",
    "editor.cursorStyle": "underline",
    "editor.wordWrap": "on",
    "editor.renameOnType": true,
    "editor.renderControlCharacters": true,
    "editor.cursorBlinking": "solid",
    "editor.formatOnSave": true,
    "editor.minimap.size": "fit",
    "editor.suggest.localityBonus": true,
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "workbench.commandPalette.history": 100,
    "workbench.commandPalette.preserveInput": true,
    "C_Cpp.clang_format_fallbackStyle": "LLVM",
    "python.analysis.memory.keepLibraryLocalVariables": true,
    "python.analysis.memory.keepLibraryAst": true,
    "python.languageServer": "Jedi",
    "code-runner.runInTerminal": true,
    "vim.cursorStylePerMode.insert": "underline",
    "vim.cursorStylePerMode.normal": "block",
    "vim.cursorStylePerMode.replace": "line",
    "vim.cursorStylePerMode.visual": "block-outline",
    "vim.cursorStylePerMode.visualblock": "block-outline",
    "vim.cursorStylePerMode.visualline": "block-outline",
    "vim.easymotion": true,
    "vim.easymotionKeys": "asdfjkl",
    "vim.easymotionMarkerBackgroundColor": "yellow",
    "vim.easymotionMarkerForegroundColorOneChar": "black",
    "vim.easymotionMarkerForegroundColorTwoChar": "black",
    "vim.easymotionMarkerFontSize": "20",
    "vim.easymotionMarkerFontWeight": "bold",
    "vim.easymotionMarkerHeight": "20",
    "vim.easymotionMarkerWidthPerChar": "10",
    "vim.hlsearch": true,
    "vim.leader": "<space>",
    "vim.searchHighlightColor": "gray",
    "workbench.editor.enablePreview": false,
    "workbench.startupEditor": "new_file.txt",
    "vim.enableNeovim": true,
    "vim.neovimPath": "/usr/bin/nvim",
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                ":"
            ],
            "commands": [
                "workbench.action.gotoLine"
            ]
        },
        {
            "before": [
                "<leader>",
                "o"
            ],
            "commands": [
                "editor.action.revealDefinitionAside"
            ]
        },
        {
            "before": [
                "<leader>",
                "<leader>",
                "o"
            ],
            "commands": [
                "editor.action.revealDefinition"
            ]
        },
        {
            "before": [
                ";"
            ],
            "commands": [
                "editor.action.showHover"
            ]
        },
        {
            "before": [
                "<leader>",
                "p"
            ],
            "commands": [
                "editor.action.peekDefinition"
            ]
        },
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        },
        {
            "before": [
                "<leader>",
                "d"
            ],
            "commands": [
                ":q"
            ]
        },
        {
            "before": [
                "<leader>",
                "w"
            ],
            "commands": [
                ":w"
            ]
        },
        {
            "before": [
                "<leader>",
                "<leader>",
                "<leader>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "B"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "E"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "f"
            ],
            "after": [
                "leader",
                "leader",
                "f"
            ]
        },
        {
            "before": [
                "F"
            ],
            "after": [
                "leader",
                "leader",
                "F"
            ]
        },
        {
            "before": [
                "t"
            ],
            "after": [
                "leader",
                "leader",
                "t"
            ]
        },
        {
            "before": [
                "T"
            ],
            "after": [
                "leader",
                "leader",
                "T"
            ]
        }
    ],
    "window.autoDetectColorScheme": true,
    "workbench.colorTheme": "Solarized Light",
    "workbench.preferredLightColorTheme": "Solarized Light",
    "workbench.preferredDarkColorTheme": "Monokai Dimmed",
    "editor.lineNumbers": "relative"
}
